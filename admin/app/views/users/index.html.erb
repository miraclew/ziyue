<table class="table">
	<tr>
		<td>username</td>
		<td>nickname</td>
		<td>balance</td>
		<td>is_locked</td>
		<td>created_at</td>
		<td></td>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.nickname %></td>
		<td><%= user.balance %></td>
		<td><%= user.is_locked %></td>
		<td><%= user.created_at %></td>
	</tr>
<% end %>
</table>	

<%= paginate @users %>